{% extends '/layout/main.html.twig' %}

{% block title %}Session{% endblock %}

{% block main %}
    <a href={{ path('app_formations') }}>Mes Formations</a>
    <h1>{{ session.getTitreComplet() }}</h1>
    <p>Session du {{ session.heureDebut|format_datetime() }} au {{ session.heureFin|format_datetime() }}</p>
    <p>{{ session.formation.description|raw }}</p>
{% endblock %}
